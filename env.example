# Configurações de Desenvolvimento
NODE_ENV=development

# OpenAI API Key (opcional - será usado se disponível)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Google Gemini API Key (recomendado)
GEMINI_API_KEY=your-gemini-api-key-here

# Grok API Key (opcional - se disponível)
GROK_API_KEY=your-grok-api-key-here

# Tavily API Key (para pesquisa web)
TAVILY_API_KEY=your-tavily-api-key-here

# JWT Secret
JWT_SECRET=your-secret-key-change-this-in-production

# Database URL (para quando usar DatabaseStorage)
DATABASE_URL=postgresql://user:password@localhost:5432/talenttracker

# Storage Configuration
USE_DATABASE=false

# Configurações do Sistema
PORT=5000
HOST=0.0.0.0 