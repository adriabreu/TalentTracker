{
  "name": "talent-tracker",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "yarn@4.9.2",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest -c jest.config.unified.cjs",
    "test:watch": "jest -c jest.config.unified.cjs --watch",
    "test:coverage": "jest -c jest.config.unified.cjs --coverage",
    "test:hooks": "jest -c jest.config.unified.cjs --testPathPatterns=hooks",
    "test:components": "jest -c jest.config.unified.cjs --testPathPatterns=components",
    "test:a11y": "jest -c jest.config.unified.cjs '.*-a11y.*\\.test\\.tsx?$'",
    "test:filters": "jest -c jest.config.unified.cjs --testPathPatterns=filters",
    "test:standardize": "node scripts/standardize-tests.js",
    "test:standardize:fix": "node scripts/standardize-tests.js --fix",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "server": "cd server && npm run dev",
    "server:build": "cd server && npm run build",
    "server:start": "cd server && npm start",
    "dev:fullstack": "concurrently \"npm run dev\" \"npm run server\"",
    "build:fullstack": "npm run build && npm run server:build",
    "start:fullstack": "npm run server:start",
    "prepare": "cd server && npm install"
  },
